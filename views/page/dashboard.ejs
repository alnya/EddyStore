<div class="row dashboardActions">
  <div class="col-sm-3"></div>
  <div class="col-sm-3">
    <div class="panel panel-default">
      <div class="panel-body">
        <span class="glyphicon glyphicon-plus"></span>
        <a href="/page/reports/add" class="btn btn-default btn-primary">New Project</a>
      </div>
    </div>
  </div>
  <div class="col-sm-3">
    <div class="panel panel-default">
      <div class="panel-body">
        <span class="glyphicon glyphicon-tasks"></span>
        <a href="/page/reports/" class="btn btn-default btn-info">Open Project</a>
      </div>
    </div>
    </div>
  <div class="col-sm-3"></div>
</div>
<div class="row" id="history">
  <div class="col-md-6">
    <div class="panel panel-info">
      <div class="panel-heading">Recent Projects</div>
      <div class="panel-body">
        <p>The last 5 processing projects you worked on</p>
      </div>
      <div class="list-group">
        <!-- ko foreach: RecentProjects -->
        <a class="list-group-item" data-bind="text: Name, attr: {'href': '/page/reports/' + id}"></a>
        <!-- /ko -->
      </div>
    </div>
  </div>
  <div class="col-md-6">
    <div class="panel panel-success">
      <div class="panel-heading">Recent Downloads</div>
      <div class="panel-body">
        <p>The last 5 processing outputs</p>
      </div>
      <div class="list-group">
        <!-- ko foreach: RecentDownloads -->
        <a class="list-group-item" data-bind="attr: {'href': '/EddyPro/' + id + '/' + id + '.zip'}">
          <span class="glyphicon glyphicon-save"></span>
          <span data-bind="text: Name"></span>
        </a>
        <!-- /ko -->
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  require(['knockout', 'jquery', 'dashboard'],
    function(ko, $, dashboard) {
      ko.applyBindings(dashboard(), $("#history")[0]);
      dashboard().init();
    });
</script>
